{
    "vim.disableAnnoyingNeovimMessage": true,
    "editor.fontFamily": "Hack",
    "editor.fontLigatures": true,
    "editor.fontSize": 13,
    "editor.minimap.enabled": false,
    "explorer.openEditors.visible": 0,
    "workbench.editor.enablePreview": false,
    "workbench.editor.tabSizing": "shrink",
    "workbench.startupEditor": "none",
    "workbench.iconTheme": "material-icon-theme",
    "symbolOutline.doSort": true,
    "symbolOutline.doSelect": false,
    "symbolOutline.sortOrder": [
        "Class",
        "Module",
        "Constant",
        "Interface",
        "*",
        "Constructor",
        "Function",
        "Method"
    ],
    "symbolOutline.expandNodes": [
        "Module",
        "Class",
        "Interface",
        "Namespace",
        "Object",
        "Package",
        "Struct"
    ],
    "symbolOutline.topLevel": [
        "*"
    ],
    "phpformatter.composer": true,
    "extensions.ignoreRecommendations": false,
    "editor.tabCompletion": true,
    "markdown.extension.showExplorer": false,
    "php-cs-fixer.config": "/Users/David/dotfiles/.php_cs",
    "material-icon-theme.showUpdateMessage": false,
    "editor.rulers": [
        80,
        120
    ],
    "workbench.statusBar.feedback.visible": false,
    "editor.renderControlCharacters": false,
    "breadcrumbs.enabled": true,
    "window.zoomLevel": 2,
    "terminal.integrated.fontFamily": "Hack",
    "python.linting.pylintArgs": [
        "--load-plugins=pylint_django"
    ],
    "gitlens.keymap": "none",
    "gitlens.currentLine.enabled": false,
    "gitlens.hovers.currentLine.enabled": false,
    "gitlens.historyExplorer.enabled": true,
    "gitlens.views.fileHistory.enabled": true,
    "gitlens.views.lineHistory.enabled": true,
    "gitlens.blame.line.enabled": false,
    "gitlens.codeLens.enabled": false,
    "gitlens.advanced.messages": {
        "suppressCommitHasNoPreviousCommitWarning": false,
        "suppressCommitNotFoundWarning": false,
        "suppressFileNotUnderSourceControlWarning": false,
        "suppressGitVersionWarning": false,
        "suppressLineUncommittedWarning": false,
        "suppressNoRepositoryWarning": false,
        "suppressResultsExplorerNotice": true,
        "suppressShowKeyBindingsNotice": true,
        "suppressUpdateNotice": false,
        "suppressWelcomeNotice": true
    },
    "php.suggest.basic": false,
    "php-cs-fixer.executablePath": "/Users/David/.composer/vendor/bin/php-cs-fixer",
    "[php]": {
        "editor.defaultFormatter": "junstyle.php-cs-fixer"
    },
    "vetur.format.defaultFormatter.js": "prettier-eslint",
    "explorer.confirmDragAndDrop": false,
    "zenMode.centerLayout": false,
    "bracket-pair-colorizer-2.colors": [
        "Orchid",
        "Green",
        "LightSkyBlue"
    ],
    "editor.wordWrap": "on",
    "explorer.compactFolders": false,
    "[javascript]": {
        "editor.tabSize": 2
    },
    "editor.detectIndentation": false,
    "editor.renderWhitespace": "none",
    "editor.formatOnSave": true,
    "workbench.colorTheme": "SynthWave '84"
}